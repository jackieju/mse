				int level = (type1 & 0x30) >> 4;
				if (level == 3)
				{
					GenError(103);
					break;
				}
				level ++;
				type1 |= level << 4;
/*				int j = (type1>>4)&0x3;
				if (j < 1)
					GenError(102);//is not a point
				else
					j--;
				j = j << 4;
*/				
