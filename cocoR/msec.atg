  (.
	int op;
	while (this->PopOp(&op))
	{		

		//操作数出栈
		long op1;
		long type1;
		TYPEDES dt1;	
		if (!m_pMainFunction->PopDigit(&op1, &type1, &dt1) ||type1 <FIRST_ADDRESS_MODE || type1 >LAST_ADDRESS_MODE)
		{
