  PreProcessor = "#" ( letter ) .
		    (.
		 	// process #include       	    		    
		char str[256];
		Scanner->GetName(&Scanner->NextSym, str, 255);
		// get directive
		char* p = strchr(str, ' ');
		char* directive = NULL;
		char* content = NULL;
		if ( p != NULL ){
			*p=0; 
			directive = str + sizeof(char);
			// proce include 
			if (strcmp(str, "include") == 0){
				// get content
				p += sizeof(char);
				while ( (*p == ' ' || *p == '\t' ) && *p != '\0' ){
					p += sizeof(char);		
				}
				if ( *p != '\0' )
					content = p;
			}

		}
			
